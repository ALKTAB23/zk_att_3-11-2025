<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Access rights for Leave Deduction Report -->
    <record id="access_leave_deduction_report" model="ir.model.access">
        <field name="name">leave.deduction.report access</field>
        <field name="model_id" ref="model_leave_deduction_report"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>

    <record id="access_leave_deduction_report_line" model="ir.model.access">
        <field name="name">leave.deduction.report.line access</field>
        <field name="model_id" ref="model_leave_deduction_report_line"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>
</odoo>
